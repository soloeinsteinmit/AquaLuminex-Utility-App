            <Label fx:id="strengthPasswordNew" alignment="CENTER_RIGHT" graphicTextGap="3.0" layoutX="72.0" layoutY="120.0" prefHeight="17.0" prefWidth="285.0" text="Strong" textAlignment="CENTER" wrapText="true">
