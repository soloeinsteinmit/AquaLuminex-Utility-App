                  <MFXPasswordField fx:id="newPasswordField" floatMode="DISABLED" layoutY="29.0" minHeight="40.0" prefHeight="40.0" prefWidth="364.0" promptText="Enter your password">
                     <font>
                        <Font name="Segoe UI Semibold" size="13.0" />
                     </font>
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                  </MFXPasswordField>
                  <Label graphicTextGap="3.0" layoutY="5.0" prefHeight="21.0" prefWidth="97.0" text="Enter password" textAlignment="CENTER" wrapText="true">
