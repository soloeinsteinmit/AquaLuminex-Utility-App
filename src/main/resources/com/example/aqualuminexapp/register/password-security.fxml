<AnchorPane fx:id="passwordAnchorPane" prefHeight="439.0" prefWidth="681.0" stylesheets="@../css/register.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.aqualuminexapp.register.PasswordSecurityController">
   <children>
